      "./dist/config.js"
    ],
    "#types": [
      "./src/types.ts",
      "./dist/types.js"
    ],
    "#core/*": [
      "./src/core/*.ts",
      "./dist/core/*.js"
    ],
    "#tables/*": [
      "./src/tables/*.ts",
      "./dist/tables/*.js"
    ],
    "#buttons/*": [
      "./src/buttons/*.ts",
      "./dist/buttons/*.js"
    ],
    "#namespaces/*": [
      "./src/namespaces/*.ts",
      "./dist/namespaces/*.js"
    ],
